#
# Chef Server Config File
#
# Dynamically generated by Chef - local modifications will be replaced
#

log_level <%= @chef_server_log_level %>
log_location <%= @chef_server_log_location == :stdout ? "STDOUT" : "'#{@chef_server_log_location}'" %>
file_store_path "<%= @chef_server_file_store_path %>"
file_cache_path "<%= @chef_server_file_cache_path %>"
ssl_verify_mode "<%= @chef_server_ssl_verify_mode %>"
registration_url "<%= @chef_server_registration_url %>"
openid_url "<%= @chef_server_openid_url %>"
template_url "<%= @chef_server_template_url %>"
remotefile_url "<%= @chef_server_remotefile_url %>"
server_search_url "<%= @chef_server_search_url %>"
cookbook_path %w{<%= @chef_server_cookbook_path.join(" ") %>}
node_path "<%= @chef_server_node_path %>"
openid_store_path "<%= @chef_server_openid_store_path %>"
openid_cstore_path "<%= @chef_server_openid_cstore_path %>"
merb_log_path "<%= @chef_server_merb_log_path %>"
search_index_path "<%= @chef_server_search_index_path %>"
openid_providers "<%= @chef_server_openid_providers %>"
<% if chef_server_show_time_in_log -%>
Chef::Log::Formatter.show_time = false
<% end -%>

